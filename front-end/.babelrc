{
  "presets": [
    [ "env", {
      "targets": {
        "browsers": ["last 2 versions"],
        "uglify": true
      },
      "modules": false
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
